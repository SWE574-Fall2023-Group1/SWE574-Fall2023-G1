{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.10.8",
    "@fortawesome/fontawesome-svg-core": "^6.4.0",
    "@fortawesome/free-solid-svg-icons": "^6.4.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@googlemaps/js-api-loader": "^1.15.1",
    "@material/textfield": "^14.0.0",
    "@mui/icons-material": "^5.11.16",
    "@mui/lab": "^5.0.0-alpha.129",
    "@mui/material": "^5.12.3",
    "@mui/styled-engine": "^5.12.0",
    "@mui/x-date-pickers": "^6.4.0",
    "@react-google-maps/api": "^2.18.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^1.3.5",
    "dayjs": "^1.11.7",
    "google-map-react": "^2.2.0",
    "history": "^5.3.0",
    "jsonwebtoken": "^9.0.0",
    "quill": "^1.3.7",
    "quill-image-compress": "^1.2.30",
    "react": "^18.2.0",
    "react-animated-heart": "^0.0.8",
    "react-dom": "^18.2.0",
    "react-icons": "^4.8.0",
    "react-quill": "^2.0.0",
    "react-router-dom": "^6.10.0",
    "react-scripts": "^5.0.1",
    "react-toastify": "^7.0.4",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts --openssl-legacy-provider start",
    "build": "react-scripts --openssl-legacy-provider build",
    "test": "react-scripts test --transformIgnorePatterns '<rootDir>/node_modules/(?!axios)'",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "testEnvironment": "node",
  "transform": {
    "^.+\\.js$": "babel-jest"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less|scss)$": "<rootDir>/styleMock.js"
    }
  },
  "devDependencies": {
    "babel-jest": "^27.4.5",
    "@babel/core": "^7.17.0",
    "@babel/preset-env": "^7.17.0"
  },
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react"
  ],
  "plugins": [
    "@babel/plugin-transform-modules-commonjs"
  ],
  "transformIgnorePatterns": [
    "/node_modules/(?!react-toastify)"
  ]
}
